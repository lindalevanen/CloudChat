{
  "expo": {
    "name": "CloudChat",
    "description": "This project is really great.",
    "slug": "CloudChat",
    "privacy": "unlisted",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "fi.vaaraj.cloudchat",
      "supportsTablet": true
    },
    "android": {
      "package": "com.mccg20",
      "googleServicesFile": "./google-services.json"
    }
  }
}
